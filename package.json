{
  "name": "@dhau/msw-builders",
  "version": "0.0.32",
  "type": "module",
  "sideEffects": false,
  "description": "Various MSW builder utils for rest and graphql",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "require.types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "import.types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier -w \"src/**/*.ts\"",
    "format:check": "prettier -c \"src/**/*.ts\"",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --ext .ts --fix ./src",
    "lint:check": "eslint --cache --cache-location ./node_modules/.cache/eslint --ext .ts ./src",
    "ts": "tsc --noEmit",
    "test": "node --watch --import tsx --test src/*.spec.ts",
    "test:all": "node --import tsx --test src/*.spec.ts",
    "jsr:check": "jsr publish --dry-run",
    "publint:check": "publint",
    "attw:check": "attw --pack .",
    "prebuild": "run-p format:check lint:check test:all jsr:check attw:check",
    "build": "tsup",
    "postbuild": "publint:check",
    "publish:npm": "npm publish --access public",
    "publish:jsr": "jsr publish --allow-slow-types",
    "deploy": "run-s build publish:*",
    "prepare": "husky install"
  },
  "author": "Daniel Holmes",
  "license": "MIT",
  "dependencies": {
    "jest-diff": "^29.3.1",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "husky": "^9.0.11",
    "jsr": "^0.12.4",
    "lint-staged": "^15.0.1",
    "msw": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "publint": "^0.2.8",
    "tslib": "^2.4.1",
    "tsup": "^8.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "msw": ">=2.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
